{
  "schemaVersion": 1,
  "id": "mapcompass",
  "version": "${version}",

  "name": "Map Compass",
  "description": "Adds a compass to maps.",
  "authors": [
    "Qendolin"
  ],
  "contact": {
    "homepage": "https://github.com/Qendolin/map-compass",
    "sources": "https://github.com/Qendolin/map-compass"
  },

  "license": "MPL-2.0",
  "icon": "assets/mapcompass/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "com.qendolin.mapcompass.Main"
    ],
    "modmenu": [
      "com.qendolin.mapcompass.ModMenuIntegration"
    ]
  },
  "mixins": [
    "mapcompass.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "minecraft": ">=1.19.3",
    "java": ">=16"
  },
  "suggests": {
    "modmenu": "*"
  }
}
